local placeId = game.PlaceId

if placeId == 70876832253163 then
    
    loadstring(game:HttpGet("https://raw.githubusercontent.com/danai1223/Magma-X/refs/heads/main/Deah%20reail"))()

elseif placeId == 987654321 then
    
    loadstring(game:HttpGet("https://example.com/scriptB.lua"))()

else
    warn("PlaceId นี้ยังไม่รองรับ:", placeId)
end
